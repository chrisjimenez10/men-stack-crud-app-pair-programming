<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Index</title>
</head>
<body>
    <h1>Game List</h1>
    <ul>
        <% videoGameList.forEach(videoGame => { %>
            <li><a href="/index/<%= videoGame._id %>"><%= videoGame.title %></a></li>
        <% }) %>
    </ul>
    <a href="/index/new"><button>Add Game</button></a>
</body>
</html>